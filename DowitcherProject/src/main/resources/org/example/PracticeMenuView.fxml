<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>


<VBox alignment="CENTER" prefHeight="373.0" prefWidth="432.0" spacing="20.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.PracticeMenuController">
   <children>
      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Practice Menu">
         <font>
            <Font size="24.0" />
         </font>
      </Text>
      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="What would you like to practice?" />
      <Button fx:id="toListenViewButton" mnemonicParsing="false" onAction="#handleToListeningButton" text="Listening" />
      <Button fx:id="toTypeViewButton" mnemonicParsing="false" onAction="#handleToTypingButton" text="Typing" />
      <Button fx:id="toTalkViewButton" mnemonicParsing="false" onAction="#handleToTalkingButton" text="Talking" />
      <Button fx:id="toTuningButton" mnemonicParsing="false" onAction="#handleToTuningButton" text="Tuning" />
      <Button fx:id="toAlphebetButton" mnemonicParsing="false" onAction="#handleToAlphebetButton" text="CW Alphebet" />
      <Button fx:id="toMainMenuButton" mnemonicParsing="false" onAction="#handleToMainMenuButton" text="Main Menu" />
   </children>
</VBox>
