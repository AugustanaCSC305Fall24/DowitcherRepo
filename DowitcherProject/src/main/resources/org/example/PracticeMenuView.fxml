<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.PracticeMenuController">
   <children>
      <Text layoutX="225.0" layoutY="46.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Practice Menu">
         <font>
            <Font size="24.0" />
         </font>
      </Text>
      <Text layoutX="214.0" layoutY="75.0" strokeType="OUTSIDE" strokeWidth="0.0" text="What would you like to practice?" />
      <VBox alignment="CENTER" layoutX="209.0" layoutY="92.0" prefHeight="261.0" prefWidth="182.0" spacing="20.0">
         <children>
            <Button fx:id="toListenViewButton" mnemonicParsing="false" onAction="#handleToListeningButton" text="Listening" />
            <Button fx:id="toTypeViewButton" mnemonicParsing="false" onAction="#handleToTypingButton" text="Typing" />
            <Button fx:id="toTalkViewButton" mnemonicParsing="false" onAction="#handleToTalkingButton" text="Talking" />
            <Button fx:id="toTuningButton" mnemonicParsing="false" onAction="#handleToTuningButton" text="Tuning" />
            <Button fx:id="toAlphebetButton" mnemonicParsing="false" onAction="#handleToAlphebetButton" text="CW Alphebet" />
